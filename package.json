{
  "name": "@tren/protocol-v1",
  "version": "1.0.1",
  "description": "Tren CDP Protocol smart contracts",
  "files": [
    "contracts",
    "artifacts"
  ],
  "scripts": {
    "run-env": "npm i && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat:goerli": "hardhat --network goerli",
    "hardhat:tenderly-main": "hardhat --network tenderlyMain",
    "hardhat:main": "hardhat --network main",
    "hardhat:fuji": "hardhat --network fuji",
    "compile": "hardhat compile",
    "console:fork": "FORK=main hardhat console",
    "dev:coverage": "buidler compile --force && buidler coverage --network coverage",
    "solhint": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "ci:test": "npm run compile && npm run test",
    "ci:clean": "rm -rf ./artifacts ./cache ./types",
    "prepublishOnly": "npm run compile"
  },
  "devDependencies": {
    "@layerzerolabs/solidity-examples": "^0.0.13",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "2.0.0",
    "@nomiclabs/buidler-etherscan": "^2.1.0",
    "@nomiclabs/buidler-waffle": "2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@tenderly/hardhat-tenderly": "1.1.0-beta.5",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@typechain/web3-v1": "1.0.0",
    "@types/chai": "4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.5",
    "bignumber.js": "9.0.0",
    "buidler-typechain": "0.1.1",
    "chai": "4.2.0",
    "chai-bignumber": "3.0.0",
    "chai-bn": "^0.2.1",
    "defender-relay-client": "^1.7.0",
    "dotenv": "^8.2.0",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "3.0.2",
    "ethereumjs-util": "7.0.2",
    "ethers": "^5.0.19",
    "globby": "^11.0.1",
    "hardhat": "^2.9.6",
    "hardhat-deploy": "0.8.9",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-gas-reporter": "^1.0.0",
    "husky": "^4.2.5",
    "lowdb": "1.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.53",
    "pretty-quick": "^2.0.1",
    "solhint": "^4.0.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.7.16",
    "temp-hardhat-etherscan": "^2.0.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "^5.1.2",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'contracts/**/*.sol' --pattern 'helpers/**/*.ts'  --pattern 'test-suites/**/*.ts' --pattern 'tasks/**/*.ts'"
    }
  },
  "author": "Tren",
  "contributors": [
    "Quinn <fury@truflation.com>"
  ],
  "license": "AGPLv3",
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "axios-curlirize": "^1.3.7",
    "tmp-promise": "^3.0.2"
  },
  "keywords": [
    "tren",
    "cdp",
    "ethereum",
    "solidity"
  ]
}
